{
  "name": "ng2elements",
  "description": "Angular Elements NodeJS Widget Server Starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run build-app-prod; npm run build-chat-prod; npm run build-passport-prod; npm run build-balance-prod; npm run build-catalogue-prod; npm run build-orders-prod; npm run build-private-orders-prod",
    "build-app-prod": "ng build --project ng-elements-app --prod --base-href /ng-elements-app/",
    "build-chat-prod": "ng build --project ng-elements-chat --prod --base-href /ng-elements-chat/",
    "build-passport-prod": "ng build --project ng-elements-passport --prod --base-href /ng-elements-passport/",
    "build-balance-prod": "ng build --project ng-elements-balance --prod --base-href /ng-elements-balance/",
    "build-catalogue-prod": "ng build --project ng-elements-catalogue --prod --base-href /ng-elements-catalogue/",
    "build-orders-prod": "ng build --project ng-elements-orders --prod --base-href /ng-elements-orders/",
    "build-private-orders-prod": "ng build --project ng-elements-private-orders --prod --base-href /ng-elements-private-orders/",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "server": "npm run build && node server.js",
    "sort-package": "npx sort-package-json",
    "start": "ng serve",
    "test": "ng test",
    "test-single-run": "ng test ng-elements-app --watch=false"
  },
  "dependencies": {
    "@angular/animations": "^7.2.1",
    "@angular/cdk": "^7.2.1",
    "@angular/common": "^7.2.1",
    "@angular/compiler": "^7.2.1",
    "@angular/core": "^7.2.1",
    "@angular/elements": "^7.2.1",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "^7.2.1",
    "@angular/http": "^7.2.1",
    "@angular/material": "^7.2.1",
    "@angular/material-moment-adapter": "^7.2.1",
    "@angular/platform-browser": "^7.2.1",
    "@angular/platform-browser-dynamic": "^7.2.1",
    "@angular/router": "^7.2.1",
    "@webcomponents/custom-elements": "^1.2.1",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "core-js": "^2.6.2",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "jwt-simple": "^0.5.5",
    "marked": "^0.6.0",
    "material-design-icon-fonts": "^3.0.1",
    "moment": "^2.23.0",
    "roboto-fontface": "^0.10.0",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.28"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.2",
    "@angular/cli": "^7.2.2",
    "@angular/compiler-cli": "^7.2.1",
    "@angular/language-service": "^7.2.1",
    "@types/jasmine": "^3.3.7",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.18",
    "codelyzer": "^4.5.0",
    "firebase": "^5.8.0",
    "firebase-tools": "^6.3.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "^5.4.2",
    "sort-package-json": "^1.17.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "typescript": "=3.1.6"
  },
  "engines": {
    "node": ">= 8.12.0",
    "npm": ">= 6.5.0"
  }
}
