{
  "name": "ng-elements-starter",
  "version": "1.0.0",
  "private": true,
  "description": "Angular Elements NodeJS Widget Server Starter",
  "scripts": {
    "build": "bash shell/build.sh",
    "build-app-prod": "ng build --project ng-elements-app --prod",
    "build-app-prod-firebase": "ng build --project ng-elements-app --prod --baseHref /ng-elements-app/",
    "build-balance-prod": "ng build --project ng-elements-balance --prod",
    "build-catalogue-prod": "ng build --project ng-elements-catalogue --prod",
    "build-firebase": "bash shell/build.sh firebase",
    "build-orders-prod": "ng build --project ng-elements-orders --prod",
    "build-passport-prod": "ng build --project ng-elements-passport --prod",
    "compodoc-generate": "compodoc -p src/tsconfig.app.json",
    "compodoc-generate-and-report-to-dist": "compodoc -p src/tsconfig.app.json && cp -r ./documentation ./dist/documentation",
    "compodoc-serve": "compodoc -p src/tsconfig.app.json -s",
    "e2e": "ng e2e",
    "e2e:headless": "ng e2e --headless --watch=false",
    "preinstall": "rm -rf node_modules/",
    "postinstall": "npx sort-package-json && npm run ngcc",
    "install-global": "bash shell/install.sh global",
    "lint": "ng lint",
    "ng": "ng",
    "ngcc": "ngcc",
    "precommit:lint": "ng lint; yarn prettier:html; yarn stylelint",
    "prettier:html": "npx prettier -c src/**/*.html",
    "prettier:html:fix": "npx prettier -c --write src/**/*.html",
    "serve-app": "npm run start-server & ng serve --project ng-elements-app",
    "serve-balance": "npm run start-server & ng serve --project ng-elements-balance",
    "serve-catalogue": "npm run start-server & ng serve --project ng-elements-catalogue",
    "serve-orders": "npm run start-server & ng serve --project ng-elements-orders",
    "serve-passport": "npm run start-server & ng serve --project ng-elements-passport",
    "preserver": "npm run build",
    "server": "node server.js",
    "start": "node server.js dev & ng serve --project ng-elements-app",
    "start-prod": "node server.js dev & ng serve --project ng-elements-app --prod",
    "start-server": "node server.js",
    "stylelint": "npx stylelint \"./src/**/*.scss\"",
    "stylelint:fix": "npx stylelint \"./src/**/*.scss\" --fix",
    "test": "ng test",
    "test-single-run": "ng test ng-elements-app --watch=false",
    "test-single-run-and-report": "ng test ng-elements-app --watch=false --code-coverage",
    "test-single-run-and-report-to-dist": "ng test ng-elements-app --watch=false --code-coverage && cp -r ./coverage ./dist/coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit:lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@angular/animations": "9.1.4",
    "@angular/cdk": "9.2.1",
    "@angular/common": "9.1.4",
    "@angular/compiler": "9.1.4",
    "@angular/core": "9.1.4",
    "@angular/elements": "9.1.4",
    "@angular/flex-layout": "9.0.0-beta.29",
    "@angular/forms": "9.1.4",
    "@angular/material": "9.2.1",
    "@angular/material-moment-adapter": "9.2.1",
    "@angular/platform-browser": "9.1.4",
    "@angular/platform-browser-dynamic": "9.1.4",
    "@angular/pwa": "0.901.4",
    "@angular/router": "9.1.4",
    "@angular/service-worker": "9.1.4",
    "@ngxs/form-plugin": "3.6.2",
    "@ngxs/store": "3.6.2",
    "@webcomponents/custom-elements": "1.4.1",
    "@webcomponents/webcomponentsjs": "2.4.3",
    "body-parser": "1.19.0",
    "components-font-awesome": "5.9.0",
    "compression": "1.7.4",
    "core-js": "3.6.5",
    "express": "4.17.1",
    "hammerjs": "2.0.8",
    "jwt-simple": "0.5.6",
    "marked": "1.0.0",
    "material-design-icon-fonts": "3.0.1",
    "moment": "2.24.0",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.5.5",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@briebug/cypress-schematic": "3.3.0",
    "@angular-builders/jest": "9.0.1",
    "@angular-devkit/build-angular": "0.901.4",
    "@angular-devkit/core": "9.1.4",
    "@angular-devkit/schematics": "9.1.4",
    "@angular-eslint/builder": "0.0.1-alpha.29",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.29",
    "@angular-eslint/eslint-plugin-template": "0.0.1-alpha.29",
    "@angular-eslint/template-parser": "0.0.1-alpha.29",
    "@angular/cli": "9.1.4",
    "@angular/compiler-cli": "9.1.4",
    "@angular/language-service": "9.1.4",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@cypress/webpack-preprocessor": "5.1.2",
    "@ngxs/devtools-plugin": "3.6.2",
    "@ngxs/logger-plugin": "3.6.2",
    "@types/jest": "25.2.1",
    "@types/jsdom": "16.2.1",
    "@types/marked": "0.7.4",
    "@types/node": "13.13.4",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "codelyzer": "5.2.2",
    "cypress": "4.5.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-compat": "3.5.1",
    "eslint-plugin-deprecation": "1.0.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-rxjs": "0.0.2-beta.20",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-watch": "6.0.1",
    "firebase": "7.14.2",
    "firebase-tools": "8.2.0",
    "husky": "4.2.5",
    "jest": "25.3.0",
    "jest-preset-angular": "8.1.3",
    "prettier": "2.0.5",
    "prettier-stylelint": "0.4.2",
    "sort-package-json": "1.42.1",
    "stylelint": "13.3.3",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-prettier": "1.1.2",
    "ts-jest": "25.3.1",
    "ts-loader": "7.0.2",
    "ts-node": "8.9.1",
    "typescript": "3.8.3",
    "web-animations-js": "2.3.2"
  },
  "engines": {
    "node": ">=12.16.1",
    "npm": ">=6.14.4"
  }
}
