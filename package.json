{
  "name": "ng2elements",
  "version": "0.0.1",
  "private": true,
  "description": "Angular Elements NodeJS Widget Server Starter",
  "scripts": {
    "build": "bash shell/build.sh",
    "build-app-prod": "ng build --project ng-elements-app --prod",
    "build-app-prod-firebase": "ng build --project ng-elements-app --prod --baseHref /ng-elements-app/",
    "build-balance-prod": "ng build --project ng-elements-balance --prod",
    "build-catalogue-prod": "ng build --project ng-elements-catalogue --prod",
    "build-dependencies-graph": "bash shell/build.sh ngd",
    "build-dependencies-graph-app": "ngd -p ./tsconfig.json -f ./src/app/app.module.ts -d ./dist/ngd/app/",
    "build-dependencies-graph-app-routing": "ngd -p ./tsconfig.json -f ./src/app/app-routing.module.ts -d ./dist/ngd/app-routing/",
    "build-dependencies-graph-balance": "ngd -p ./tsconfig.json -f ./src/app/elements/balance/balance.module.ts -d ./dist/ngd/balance/",
    "build-dependencies-graph-catalogue": "ngd -p ./tsconfig.json -f ./src/app/elements/catalogue/catalogue.module.ts -d ./dist/ngd/catalogue/",
    "build-dependencies-graph-orders": "ngd -p ./tsconfig.json -f ./src/app/elements/orders/orders.module.ts -d ./dist/ngd/orders/",
    "build-dependencies-graph-passport": "ngd -p ./tsconfig.json -f ./src/app/elements/passport/passport.module.ts -d ./dist/ngd/passport/",
    "build-firebase": "bash shell/build.sh firebase",
    "build-orders-prod": "ng build --project ng-elements-orders --prod",
    "build-passport-prod": "ng build --project ng-elements-passport --prod",
    "e2e": "ng e2e",
    "generate-ngd-index": "bash shell/generate-ngd-index.sh",
    "postinstall": "npm run sort-package",
    "install-global": "bash shell/install.sh global",
    "lint": "ng lint",
    "ng": "ng",
    "serve-app": "npm run start-server & ng serve --project ng-elements-app",
    "serve-balance": "npm run start-server & ng serve --project ng-elements-balance",
    "serve-catalogue": "npm run start-server & ng serve --project ng-elements-catalogue",
    "serve-orders": "npm run start-server & ng serve --project ng-elements-orders",
    "serve-passport": "npm run start-server & ng serve --project ng-elements-passport",
    "preserver": "npm run build",
    "server": "node server.js",
    "sort-package": "npx sort-package-json",
    "start": "node server.js dev & ng serve --project ng-elements-app",
    "start-prod": "node server.js dev & ng serve --project ng-elements-app --prod",
    "start-server": "node server.js",
    "test": "ng test",
    "test-single-run": "ng test ng-elements-app --watch=false",
    "test-single-run-and-report": "ng test ng-elements-app --watch=false --code-coverage",
    "test-single-run-and-report-to-dist": "ng test ng-elements-app --watch=false --code-coverage && cp -r ./coverage ./dist/coverage"
  },
  "dependencies": {
    "@angular/animations": "^7.2.6",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "^7.2.6",
    "@angular/compiler": "^7.2.6",
    "@angular/core": "^7.2.6",
    "@angular/elements": "^7.2.6",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "^7.2.6",
    "@angular/http": "^7.2.6",
    "@angular/material": "^7.3.3",
    "@angular/material-moment-adapter": "^7.3.3",
    "@angular/platform-browser": "^7.2.6",
    "@angular/platform-browser-dynamic": "^7.2.6",
    "@angular/router": "^7.2.6",
    "@webcomponents/custom-elements": "^1.2.1",
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "core-js": "^2.6.5",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "jwt-simple": "^0.5.5",
    "marked": "^0.6.1",
    "material-design-icon-fonts": "^3.0.1",
    "moment": "^2.24.0",
    "roboto-fontface": "^0.10.0",
    "rxjs": "^6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.3",
    "@angular/cli": "^7.3.3",
    "@angular/compiler-cli": "^7.2.6",
    "@angular/language-service": "^7.2.6",
    "@types/jasmine": "^3.3.9",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^11.9.5",
    "codelyzer": "^4.5.0",
    "firebase": "^5.8.4",
    "firebase-tools": "^6.4.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "^5.4.2",
    "sort-package-json": "^1.19.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "=3.1.6"
  },
  "engines": {
    "node": ">= 8.12.0",
    "npm": ">= 6.8.0"
  }
}
