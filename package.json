{
  "name": "ng-elements-starter",
  "version": "1.0.0",
  "private": true,
  "description": "Angular Elements NodeJS Widget Server Starter",
  "scripts": {
    "build": "bash shell/build.sh",
    "build-app-prod": "ng build --project ng-elements-app --prod",
    "build-app-prod-firebase": "ng build --project ng-elements-app --prod --baseHref /ng-elements-app/",
    "build-balance-prod": "ng build --project ng-elements-balance --prod",
    "build-catalogue-prod": "ng build --project ng-elements-catalogue --prod",
    "build-firebase": "bash shell/build.sh firebase",
    "build-orders-prod": "ng build --project ng-elements-orders --prod",
    "build-passport-prod": "ng build --project ng-elements-passport --prod",
    "compodoc-generate": "compodoc -p src/tsconfig.app.json",
    "compodoc-generate-and-report-to-dist": "compodoc -p src/tsconfig.app.json && cp -r ./documentation ./dist/documentation",
    "compodoc-serve": "compodoc -p src/tsconfig.app.json -s",
    "preprotractor": "npm run update-webdriver",
    "protractor": "node server.js dev & protractor e2e/protractor.conf.js",
    "e2e": "ng e2e",
    "preinstall": "rm -rf node_modules/",
    "postinstall": "npx sort-package-json && npm run ngcc",
    "install-global": "bash shell/install.sh global",
    "lint": "ng lint",
    "ng": "ng",
    "ngcc": "ngcc",
    "prettier:html": "npx prettier -c src/**/*.html",
    "prettier:html:fix": "npx prettier -c --write src/**/*.html",
    "serve-app": "npm run start-server & ng serve --project ng-elements-app",
    "serve-balance": "npm run start-server & ng serve --project ng-elements-balance",
    "serve-catalogue": "npm run start-server & ng serve --project ng-elements-catalogue",
    "serve-orders": "npm run start-server & ng serve --project ng-elements-orders",
    "serve-passport": "npm run start-server & ng serve --project ng-elements-passport",
    "preserver": "npm run build",
    "server": "node server.js",
    "start": "node server.js dev & ng serve --project ng-elements-app",
    "start-prod": "node server.js dev & ng serve --project ng-elements-app --prod",
    "start-server": "node server.js",
    "stylelint": "npx stylelint \"./src/**/*.scss\"",
    "stylelint:fix": "npx stylelint \"./src/**/*.scss\" --fix",
    "test": "ng test",
    "test-single-run": "ng test ng-elements-app --watch=false",
    "test-single-run-and-report": "ng test ng-elements-app --watch=false --code-coverage",
    "test-single-run-and-report-to-dist": "ng test ng-elements-app --watch=false --code-coverage && cp -r ./coverage ./dist/coverage",
    "update-webdriver": "webdriver-manager update"
  },
  "dependencies": {
    "@angular/animations": "9.1.0",
    "@angular/cdk": "9.2.0",
    "@angular/common": "9.1.0",
    "@angular/compiler": "9.1.0",
    "@angular/core": "9.1.0",
    "@angular/elements": "9.1.0",
    "@angular/flex-layout": "9.0.0-beta.29",
    "@angular/forms": "9.1.0",
    "@angular/material": "9.2.0",
    "@angular/material-moment-adapter": "9.2.0",
    "@angular/platform-browser": "9.1.0",
    "@angular/platform-browser-dynamic": "9.1.0",
    "@angular/pwa": "0.901.0",
    "@angular/router": "9.1.0",
    "@angular/service-worker": "9.1.0",
    "@ngxs/store": "3.6.2",
    "@webcomponents/custom-elements": "1.4.1",
    "@webcomponents/webcomponentsjs": "2.4.3",
    "body-parser": "1.19.0",
    "components-font-awesome": "5.9.0",
    "compression": "1.7.4",
    "core-js": "3.6.4",
    "express": "4.17.1",
    "hammerjs": "2.0.8",
    "jwt-simple": "0.5.6",
    "marked": "0.8.2",
    "material-design-icon-fonts": "3.0.1",
    "moment": "2.24.0",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.5.5",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.7",
    "@angular-devkit/core": "9.1.0",
    "@angular-devkit/schematics": "9.1.0",
    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "9.1.0",
    "@angular/language-service": "9.1.0",
    "@ngxs/devtools-plugin": "3.6.2",
    "@ngxs/logger-plugin": "3.6.2",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "13.11.0",
    "codelyzer": "5.2.2",
    "eslint": "6.8.0",
    "firebase": "7.13.2",
    "firebase-tools": "8.0.1",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.3",
    "prettier": "2.0.2",
    "prettier-stylelint": "0.4.2",
    "protractor": "6.0.0",
    "sort-package-json": "1.40.0",
    "stylelint": "13.3.0",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-prettier": "1.1.2",
    "ts-node": "8.8.2",
    "tslint": "6.1.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-etc": "1.10.1",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "3.8.3",
    "web-animations-js": "2.3.2"
  },
  "engines": {
    "node": ">=10.16.0",
    "npm": ">=6.9.0"
  }
}
